@model IEnumerable<SpotifyWebAPI.Artist>

@{
    ViewBag.Title = "SearchAsync";
    Layout = "~/Views/Shared/_ArtistLayout.cshtml";
}

<h2>SearchAsync</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HREF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Popularity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Uri)
        </th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HREF)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Popularity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Uri)
        </td>
    </tr>
}
</table>
